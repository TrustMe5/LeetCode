#!/usr/bin/env python
# coding=utf-8
def processnum(self,str):
    num='0123456789'
    length=len(str)
    if length==0:
        return 0
    if str[0]=='-' or str[0]=='+':
        i=1
        while i<length and str[i] in num:
            i++
        if i==1:
            return 0
        else:
            integer=int(str[:i])

